(set-logic QF_BV)
;(set-info :source | 10人站队，甲不能站两端，与乙相邻，与丙隔两个人，且在丁后面，问这四人有多少种站队方法 | )
(declare-fun xA () (_ BitVec 4))
(declare-fun xB () (_ BitVec 4))
(declare-fun xC () (_ BitVec 4))
(declare-fun xD () (_ BitVec 4))
(assert (and (bvugt xA #b0001) (bvult xA #b1010)))
(assert (and (bvuge xB #b0001) (bvule xB #b1010)))
(assert (and (bvuge xC #b0001) (bvule xC #b1010)))
(assert (and (bvuge xD #b0001) (bvule xD #b1010)))
(assert (and (not (= xA xB)) (not (= xA xC)) (not (= xA xD)) (not (= xB xC)) (not (= xB xD)) (not (= xC xD))))
(assert (or (= xA (bvsub xB #b0001)) (= xA (bvadd xB #b0001))))
(assert (or (= (bvsub xA xC) #b0011) (= (bvsub xC xA) #b0011)))
(assert (bvugt xA xD))
(check-sat)
;(get-model)
(exit)
