(set-logic QF_BV)
(declare-fun a () (_ BitVec 8))
(declare-fun b () (_ BitVec 8))
(declare-fun c () (_ BitVec 8))
(assert(not (or (and (bvslt a b) (bvslt b c)) (and (bvslt c b) (bvslt b a)))))
(assert(not (or (and (bvslt a c) (bvslt c b)) (and (bvslt b c) (bvslt c a)))))
(assert(or (and (bvslt b a) (bvslt a c)) (and (bvslt c a) (bvslt a b))))
(check-sat) 
